{
    "name": "raehalme/oauth2-keycloak",
    "description": "Keycloak OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Raehalme",
            "email": "thomas.raehalme@aitiogroup.com",
            "homepage": "https://github.com/AitioGroup"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "keycloak"
    ],
    "require": {
        "php": ">=5.5.0",
        "league/oauth2-client": "1.0.0-beta2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Raehalme\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Raehalme\\OAuth2\\Client\\Test\\": "test/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
